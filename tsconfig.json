{
    "compilerOptions": {
        "target": "es2021",
        "lib": ["es2021"],
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "strictNullChecks": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "noEmit": true
    }
}
